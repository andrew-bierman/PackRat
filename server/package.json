    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/generated/test/**/*.[jt]s?(x)",
      "<rootDir>/generated/test/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.[jt]s?(x)",
