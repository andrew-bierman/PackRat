{
  "package": {
    "productName": "Packrat",
    "version": "1.0.0",
    "identifier": "com.andrewbierman.packrat"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:4200",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn run build",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "tauri": {
    "windows": [
      {
        "title": "Packrat",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": ["all"]
    }
  }
}
