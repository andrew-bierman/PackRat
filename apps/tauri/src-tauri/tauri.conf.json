{
  "package": {
    "productName": "Packrat",
    "version": "1.0.0"
  },
  "build": {
    "distDir": "../dist",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn run build"
  },
  "tauri": {
    "windows": [
      {
        "title": "Packrat",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "identifier": "com.andrewbierman.packrat",
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
