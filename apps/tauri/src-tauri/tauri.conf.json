{
  "$schema": "../../../node_modules/@tauri-apps/cli/schema.json",
  "package": {
    "productName": "PackRat",
    "version": "0.1.0"
  },
  "build": {
    "beforeBuildCommand": "yarn run build",
    "beforeDevCommand": "yarn run dev"
  },

  "tauri": {
    "bundle": {
      "identifier": "com.andrewbierman.packrat",
      "active": true,
      "category": "DeveloperTool",
      "targets": "all",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "digestAlgorithm": "sha256",
        "certificateThumbprint": "",
        "timestampUrl": ""
      },
      "macOS": {
        "entitlements": "",
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": "",
        "signingIdentity": ""
      }
    },
    "security": {
      "csp": null
    }
  }
}
